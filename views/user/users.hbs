<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>Azonosító</th>
            <th>Név</th>
            <th>Cím</th>
            <th>E-mail</th>
            <th>Opciók</th>
        </tr>
    </thead>
    <tbody>
        {{#each users as |user|}}
        <tr  class="active">
            <td>{{user.id}}</td>
            <td>{{user.firstname}} {{user.lastname}}</td>
            <td>{{user.zipcode}} {{user.city}} {{user.address}}</td>
            <td>{{user.email}}</td>
            <td>
                <a class="btn btn-sm btn-danger" href="/user/deleteuser/{{user.id}}">
                    <span class="glyphicon glyphicon-trash"></span>
                </a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table> 
<a href="/" class="btn btn-primary">Vissza</a>